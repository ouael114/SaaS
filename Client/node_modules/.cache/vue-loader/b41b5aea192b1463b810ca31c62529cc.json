:{\r\n      required\r\n    },\r\n  },\r\n  methods: {\r\n    submitForm() {\r\n      this.$v.$touch();\r\n      if (!this.$v.$invalid) {\r\n          axios.put('http://localhost:3000/companies/add-member', {\r\n           adminemail:this.adminEmail,\r\n           fullname:this.name,\r\n           email:this.email,\r\n           password:this.password,\r\n           companyname:this.companyname\r\n            })\r\n          .then(response=> console.log(response))\r\n          .catch(error=> console.log(error))\r\n      }\r\nalert(\"User Submited\")\r\n     \r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n</style>"]}]}