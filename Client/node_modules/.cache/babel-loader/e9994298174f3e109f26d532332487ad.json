sources":["AddTeamForm.vue"],"sourcesContent":["<template>\r\n  <form @submit.prevent=\"addmember\">\r\n    <md-card>\r\n      <md-card-header :data-background-color=\"dataBackgroundColor\">\r\n        <h4 class=\"title\">Add Team Member</h4>\r\n        <p class=\"category\">Complete your profile</p>\r\n      </md-card-header>\r\n      <md-card-content>\r\n        <div class=\"md-layout\">\r\n       \r\n          <div class=\"md-layout-item md-small-size-100 md-size-33\">\r\n            <md-field>\r\n              <label>Admin Email</label>\r\n              <md-input v-model=\"FormData.adminemail\" type=\"text\" name=\"adminemail\"></md-input>\r\n            </md-field>\r\n          </div>\r\n          <div class=\"md-layout-item md-small-size-100 md-size-33\">\r\n            <md-field>\r\n              <label>Name</label>\r\n              <md-input v-model=\"FormData.name\" type=\"text\" name=\"name\"></md-input>\r\n            </md-field>\r\n          </div>\r\n           <div class=\"md-layout-item md-small-size-100 md-size-33\">\r\n            <md-field>\r\n              <label>Surname</label>\r\n              <md-input v-model=\"FormData.surname\" type=\"text\" name=\"surname\"></md-input>\r\n            </md-field>\r\n          </div>\r\n          <div class=\"md-layout-item md-small-size-100 md-size-33\">\r\n            <md-field>\r\n              <label>Email</label>\r\n              <md-input v-model=\"FormData.email\" type=\"text\" name=\"email\"></md-input>\r\n            </md-field>\r\n          </div>\r\n          <div class=\"md-layout-item md-small-size-100 md-size-50\">\r\n            <md-field>\r\n              <label>Company Name</label>\r\n              <md-input v-model=\"FormData.companyname\" type=\"text\" name=\"companyname\"></md-input>\r\n            </md-field>\r\n          </div>\r\n          <div class=\"md-layout-item md-small-size-100 md-size-50\">\r\n            <md-field>\r\n              <label>Password</label>\r\n              <md-input v-model=\"FormData.password\" type=\"password\" name=\"password\"></md-input>\r\n            </md-field>\r\n          </div>\r\n          <div class=\"md-layout-item md-size-100 text-right\">\r\n            <button class=\"btn btn-success\"> Submit</button>\r\n          </div>\r\n        </div>\r\n      </md-card-content>\r\n    </md-card>\r\n  </form>\r\n</template>\r\n<script>\r\nimport axios from 'axios'\r\nexport default {\r\n  name: \"add-team-form\",\r\n  methods: {\r\n      addmember(){\r\n          axios.put('http://localhost:3000/companies/add-member',this.FormData)\r\n          .then(response=> console.log(response))\r\n          .catch(error=> console.log(error))\r\n      }\r\n  },\r\n  props: {\r\n    dataBackgroundColor: {\r\n      type: String,\r\n      default: \"\",\r\n    },\r\n  },\r\n  data() {\r\n    return {\r\n      FormData:{\r\n        adminemail:'',\r\n        name:'',\r\n        surname:'',\r\n        email:'',\r\n        companyname:'',\r\n        password:'',\r\n        role:''}\r\n    };\r\n  },\r\n};\r\n</script>\r\n<style></style>\r\n\r\n\r\n"]}]}