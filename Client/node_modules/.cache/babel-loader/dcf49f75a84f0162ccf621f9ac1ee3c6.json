ents/Tables","sources":["CompanyTable.vue"],"sourcesContent":["<template>\n  <body>\n    <div class=\"container mt-4\" id=\"app\">\n      <table class=\"table table-border\">\n        <thead>\n          <tr>\n            <th scope=\"col\">#</th>\n            <th scope=\"col\">Companie Name</th>\n            <th scope=\"col\">Admin Email</th>\n            <th scope=\"col\">Web Site</th>\n            <th scope=\"col\">License</th>\n            <th scope=\"col\">Package</th>\n            <th scope=\"col\">Action</th>\n          </tr>\n        </thead>\n        <tbody>\n          <tr v-for=\"(companie, _id) in companies\" :key=\"_id\">\n          <th scope=\"row\"></th>\n          <td>{{ companie.nom }}</td>\n          <td>{{ companie.adminEmail }}</td>\n          <td>{{ companie.siteWeb }}</td>\n          <td>{{ companie.license }}</td>\n          <td>{{ companie.pack }}</td>\n          <td>\n            <button @click=\"deletecompany(companie._id)\" class=\"btn btn-sm btn-outline-danger js-sweetalert\">Delete</button>\n          </td>\n          </tr>\n        </tbody>\n      </table>\n    </div>\n  </body>\n</template>\n\n<script>\nimport axios from \"axios\";\nexport default {\n  data(){\n    return{\n      companies:[]\n    }\n  },\n  name: \"company-table\",\n  mounted: function () {\n    axios\n      .get(\"http://localhost:3000/users/getcompanie\")\n      .then((response) => {\n        this.companies = response.data.companyData;\n        console.log(response);\n      })\n      .catch((error) => {\n        console.log(error);\n      });\n  },\n    methods:{\n    deletecompany(_id)\n        {\n         axios.delete(\"http://localhost:3000/company/\"+_id)\n         this.function()\n         .then(response=> console.log(response))\n          .catch(error=> console.log(error))\n        },\n  },\n  props: {\n    tableHeaderColor: {\n      type: String,\n      default: \"\",\n    },\n  },\n};\n</script>\n"]}]}